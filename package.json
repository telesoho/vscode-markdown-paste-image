{
  "name": "vscode-markdown-paste-image",
  "displayName": "Markdown Paste Image",
  "description": "Paste image from clipboard directly",
  "version": "0.4.9",
  "publisher": "telesoho",
  "author": {
    "name": "telesoho",
    "email": "telesoho@gmail.com"
  },
  "license": "(MIT)",
  "repository": {
    "type": "git",
    "url": "https://github.com/telesoho/vscode-markdown-paste-image.git"
  },
  "homepage": "https://github.com/telesoho/vscode-markdown-paste-image",
  "bugs": {
    "url": "https://github.com/telesoho/vscode-markdown-paste-image/issues"
  },
  "icon": "res/icon.png",
  "galleryBanner": {
    "color": "#252526",
    "theme": "dark"
  },
  "keywords": [
    "paste",
    "image",
    "markdown",
    "paste image",
    "keybindings"
  ],
  "engines": {
    "vscode": "^1.0.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.MarkdownPasteImage"
  ],
  "main": "./out/src/extension",
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Markdown Paste Image Configuration",
      "properties": {
        "pasteImage.path": {
          "type": "string",
          "default": "./",
          "description": "The destination to save image file."
        },
        "pasteImage.silence" : {
          "type" : "boolean",
          "default" : false,
          "description" : "Enable/disable showing confirm box while paste image."
        }
      }
    },
    "commands": [
      {
        "command": "extension.MarkdownPasteImage",
        "title": "Markdown Paste Image"
      }
    ],
    "keybindings": [
      {
        "command": "extension.MarkdownPasteImage",
        "key": "ctrl+alt+v",
        "mac": "cmd+alt+v",
        "when": "editorTextFocus"
      }
    ]
  },
  "dependencies": {
    "copy-paste": "^1.2.0",
    "moment": "^2.14.1",
    "shelljs": "^0.7.7"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.40",
    "mocha": "^2.3.3",
    "shelljs": "^0.7.7",
    "typescript": "^2.0.3",
    "vscode": "^1.0.0"
  }
}
